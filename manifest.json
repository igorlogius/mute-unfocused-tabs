{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius/mute-unfocused-tabs",
  "description": "Prevent distractions by only allowing the focused tab to be audible",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_area": "navbar",
    "default_icon": "icon.png"
  },
  "icons": {
    "320": "icon.png"
  },
  "manifest_version": 2,
  "name": "Mute Unfocused Tabs",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": ["tabs", "storage", "menus"],
  "version": "1.4.21"
}
